{
  "rules": {
    "block-scoped-var": 2,
    "block-spacing": 2,
    "camelcase": [2, {"properties": "never"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": 2,
    "consistent-return": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 1,
    "eqeqeq": 2,
    "indent": [2, "tab"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "linebreak-style": [2, "unix"],
    "no-eval": 2,
    "no-extend-native": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-console": 0,
    "no-invalid-this": 2,
    "no-process-exit": 2,
    "no-unexpected-multiline": 1, "semi": [2, "always"],
    "quotes": [2, "single"],
    "vars-on-top": 2,
    "valid-jsdoc": [1, {"requireReturn": false, "requireReturnDescription": false, "requireParamDescription": false}],
    "wrap-iife": [2, "outside"]
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}


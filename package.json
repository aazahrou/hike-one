{
  "name": "hike-one",
  "version": "1.0.0",
  "description": "website for Hike One",
  "main": "src/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "serve dist --single --cache 1",
    "prebuild": "rimraf dist && mkdirp dist",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --host 0.0.0.0 --watch-poll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/hike-one.git"
  },
  "author": "De Voorhoede",
  "license": "All Rights Reserved",
  "private": true,
  "dependencies": {
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "preact-router": "2.5.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.0",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "mkdirp": "0.5.1",
    "postcss-loader": "2.0.5",
    "raw-loader": "0.5.1",
    "replace-bundle-webpack-plugin": "1.0.0",
    "rimraf": "2.6.1",
    "serve": "5.2.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  }
}
